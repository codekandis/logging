{
	"name": "codekandis/logging",
	"description": "`codekandis/logging` is a library providing logging interfaces and classes respecting the interfaces of `psr/log`.",
	"keywords": [
		"php",
		"logging",
		"library",
		"interfaces",
		"classes",
		"psr",
		"log"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/logging/issues",
		"docs": "https://github.com/codekandis/logging/README.md",
		"source": "https://github.com/codekandis/logging/tree/master"
	},
	"require": {
		"php": "^7.4"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\Logging\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\Logging\\Tests\\": [
				"tests/"
			]
		}
	}
}
